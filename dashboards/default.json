{
  "name": "simple-dashboard",
  "layout": {
    "columns": 6,
    "rows": 4
  },
  "plugins": {
    "ds-cryptocompare": { 
      "module": "plugins/cryptocompare-plugin"
    }
  },
  "widgets": [
    {
      "position": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 1
      },
      "widget": "vudash-widget-time"
    },
    {
      "position": {
        "x": 0,
        "y": 1,
        "w": 3,
        "h": 2
      },
      "datasource": {
        "name": "ds-cryptocompare",
        "options": {
          "symbol": "BTC"
        }
      },
      "options": {
        "schedule": 86400000,
        "description": "Bitcoin Price (GBP) / days ago",
        "labels": [7,6,5,4,3,2,1]
      },
      "widget": "@vudash/widget-chart"
    },
    {
      "position": {
        "x": 3,
        "y": 1,
        "w": 3,
        "h": 2
      },
      "datasource": {
        "name": "ds-cryptocompare",
        "options": {
          "symbol": "ETH"
        }
      },
      "options": {
        "schedule": 86400000,
        "description": "Ethereum Price (GBP) / days ago",
        "labels": [7,6,5,4,3,2,1]
      },
      "widget": "@vudash/widget-chart"
    }
  ]
}
